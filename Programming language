import java.util.Scanner;
public class RemoveVowels {
    public static void main(String[]args) {
        String a="programming language";
        String b="";
        for(int i=0;i<a.length();i++) {
            char ch=a.charAt(i);
            if((ch>='a'&&ch<='z')) {
                b+=ch;
            }
        }
        
            System.out.println("enter the programming language:"+b);
    }
}   
            
